<% for(var i=0; i<server.online.length; i++) { %>
    <tr>
        <td><%= i+1 %></td>
        <td><a href="http://steamcommunity.com/profiles/<%= server.online[i].steamid %>" target="_blank"><%= server.online[i].steamid %></a></td>
        <td><%= server.online[i].name %></td>
        <td><%= server.online[i].ip %></td>
        <td><%= server.online[i].ping %></td>
        <td><%= server.online[i].played %></td>
        <td>
            <% if (roleCheck('kick')) { %>
                <button type="button" class="btn btn-primary ajaxRequest" data-type="kick" data-name="<%= server.online[i].name %>">Kick</button>
            <% } %>
            <% if (roleCheck('ban')) { %>
                <a href="#banModal" class="btn btn-danger modalSpawn" data-name="<%= server.online[i].name %>">Ban</a>
            <% } %>
            <% if (roleCheck('give')) { %>
                <a href="#giveModal" class="btn btn-info modalSpawn" data-name="<%= server.online[i].name %>">Give</a>
            <% } %>
            <% if (roleCheck('teleport')) { %>
                <a href="#teleportModal" class="btn btn-default modalSpawn" data-name="<%= server.online[i].name %>">Tele</a>
            <% } %>
        </td>
    </tr>
<% } %>